{
	"@meta": {
		"is_molang": true,
		"validate": "molang.js"
	},
	"$versioned_template": [
		{
			"$if": "$molang_features",
			"$data": "('query.' + $molang.general.query_beta)"
		},
		{
			"$if": "$format_version >= 1.20.20",
			"$data": "('query.' + $molang.general.query_post_1.20.20)"
		},
		{
			"$if": "$format_version < 1.20.20",
			"$data": "('query.' + $molang.general.query_pre_1.20.20)"
		},
		{
			"$data": "$molang.general.boolean and ('query.' + $molang.general.query) and ('variable.' + $molang.general.variable) and ('context.' + $molang.general.context)"
		}
	]
}