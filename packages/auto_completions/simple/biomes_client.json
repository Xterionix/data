{
    "biomes": {
        "default": {
            "water_surface_color": {
                "@meta": {
                    "is_color": true,
                    "is_value": true
                }
            },
            "water_surface_transparency": "$general.decimal",
            "$versioned_template": [
                {
                    "$if": "$project_target_version >= 1.16.100",
                    "$data": {
                        "fog_identifier": "$general.fog_identifier"
                    }
                },
                {
                    "$if": "$project_target_version < 1.16.100",
                    "$data": {
                        "water_fog_color": {
                            "@meta": {
                                "is_color": true,
                                "is_value": true
                            }
                        },
                        "water_fog_distance": "$general.number",
                        "fog_color": {
                            "@meta": {
                                "is_color": true,
                                "is_value": true
                            }
                        }
                    }
                }
            ]
        },
        "$placeholder": "$simple.biomes_client.biomes.default"
    }
}