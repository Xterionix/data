{
	"event_definition": {
		"event": {
			"@meta": {
				"is_value": true
			},
			"@import.value": "$dynamic.block.events"
		},
		"target": [
			"self",
			"other",
			"target",
			"parent",
			"baby"
		],
		"condition": "$molang.embedded"
	},
	"block_faces": [
		"up",
		"down",
		"side",
		"north",
		"east",
		"west",
		"south"
	],
	"components": "$block.components and $dynamic.plugins.block_custom_components"
}